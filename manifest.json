{
  "manifest_version": 3,
  "name": "Rested",
  "version": "1.0",
  "description": "Save and close tabs to reduce memory usage.",
  "permissions": ["tabs", "storage"],
  "background": {
    "service_worker": "background.js"
  },
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "images/rested16.png",
      "48": "images/rested48.png",
      "128": "images/rested128.png"
    }
  },
  "icons": {
    "16": "images/rested16.png",
    "48": "images/rested48.png",
    "128": "images/rested128.png"
  }
}
